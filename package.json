{
  "name": "resume",
  "version": "1.0.0",
  "description": "resume online",
  "main": "server/index.js",
  "author": "Jordane Michon <jordane.michon@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "start:prod": "npm run build && npm run serve",
    "serve": "cross-env NODE_ENV=production node buildServer/index.js",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "rimraf buildClient && cross-env NODE_ENV=production webpack --mode production -p --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && cross-env NODE_ENV=production webpack --mode production -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server/index.js -o buildServer/index.js",
    "clean": "rimraf buildClient buildServer",
    "precommit": "lint-staged",
    "cm": "git-cz",
    "lint": "eslint --fix src server webpack",
    "format": "prettier --single-quote --trailing-comma es5 --write '{src,server,webpack}/**/*.js' && npm run lint",
    "extract:messages": "react-intl-cra 'src/**/*.{js,jsx}' -o 'src/i18n/messages/messages.json'",
    "manage:translations": "node ./translationRunner.js"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "aws-sdk": "^2.386.0",
    "babel-plugin-styled-components": "^1.9.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.6.1",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "emotion": "^10.0.6",
    "emotion-server": "^10.0.6",
    "express": "^4.15.2",
    "file-loader": "^2.0.0",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.6.3",
    "joi": "^14.3.1",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-burger-menu": "^2.5.4",
    "react-circular-progressbar": "^1.0.0",
    "react-dom": "^16.6.3",
    "react-easy-swipe": "^0.0.17",
    "react-ga": "^2.5.7",
    "react-intl": "^2.7.2",
    "react-intl-cra": "^0.3.3",
    "react-intl-translations-manager": "^5.0.3",
    "react-redux": "^6.0.0",
    "react-select": "^2.1.2",
    "react-transition-group": "^2.5.1",
    "react-universal-component": "^3.0.3",
    "rebass": "^3.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.15",
    "redux-first-router-link": "^1.1.4",
    "styled-components": "^4.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack-flush-chunks": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-universal-import": "^3.1.2",
    "babel-watch": "^7.0.0",
    "bluebird": "^3.5.1",
    "commitizen": "^2.9.6",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^1.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.4.4",
    "react-hot-loader": "^4.6.2",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.7.0",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --single-quote --trailing-comma es5 --write",
        "eslint --fix",
        "git add"
      ]
    }
  }
}
